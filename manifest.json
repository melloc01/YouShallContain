{
  "manifest_version": 2,
  "name": "You Shall Contain!",
  "version": "0.1",

  "description": "",
  "icons": {
    "48": "img/wizard.png",
    "96": "img/wizard.png"
  },

  "applications": {
    "gecko": {
      "strict_min_version": "53.0"
    }
  },

  "permissions": [
    "<all_urls>",
    "contextualIdentities",
    "storage",
    "tabs",
    "webRequest"
  ],

  "browser_action": {
    "browser_style": true,
    "default_title": "You Shall Contain!",
    "default_popup": "popup.html",
    "default_icon": "img/wizard.png"
  },
  "background": {
    "page": "popup.html"
  }
}
